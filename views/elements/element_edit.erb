<h1>Edit Element</h1>

<!--
  The request created by this form's submission will go to
  "/users/:id/edit" and the HTTP verb will be "post".
-->
<form action="/admin/<%= @element.id %>/update_element" method="post">
  <p>Name: <input type="text" name="name" value="<%= @element.name %>"></p>
  
  <p>Weight: <input type="text" name="weight" value="<%= @element.weight %>"></p>
  
  <p> Comic: <input type="text" name="comic" value="<%= @element.comic %>">
  
 
    <input type="submit">
</form>